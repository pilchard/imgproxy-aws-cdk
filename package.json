{
	"name": "imgproxy-aws-cdk",
	"version": "0.1.0",
	"type": "module",
	"targets": {
		"url-rewrite": {
			"source": "functions/url-rewrite/index.ts",
			"distDir": ".dist/functions/url-rewrite",
			"context": "node",
			"isLibrary": true,
			"outputFormat": "esmodule",
			"includeNodeModules": false,
			"scopeHoist": true,
			"sourceMap": false,
			"optimize": true,
			"engines": {
				"node": ">= 20"
			}
		},
		"main": false
	},
	"bin": {
		"imgproxy": "./bin/imgproxy.js"
	},
	"scripts": {
		"deploy": "cdk deploy",

		"destroy": "cdk destroy --force",
		"postdestroy": "tsx ./bin/imgproxy-post.ts destroy",
		"typecheck": "tsc --noEmit",
		"build": "pnpm run typecheck && rm -rf ./.dist && pnpm run build:parcel",
		"build:parcel": "parcel build",
		"test:functions": "vitest"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@parcel/config-default": "^2.14.4",
		"@parcel/plugin": "^2.14.4",
		"@pilchard/aws-cloudfront-function": "link:../aws-cloudfront-function",
		"@types/aws-lambda": "^8.10.149",
		"@types/node": "22.13.5",
		"@types/prompts": "^2.4.9",
		"ansis": "^3.17.0",
		"cloudfront": "link:functions/url-rewrite/__mocks__/cloudfront",
		"dotenv": "^16.5.0",
		"execa": "^9.5.2",
		"parcel": "^2.14.4",
		"prompts": "^2.4.2",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"typescript": "5.8.2",
		"vite": "^6.3.2",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"aws-cdk": "2.1010.0",
		"aws-cdk-lib": "2.190.0",
		"constructs": "10.4.2",
		"source-map-support": "^0.5.21",
		"cloudfront": "link:functions/url-rewrite/__mocks__/cloudfront"
	},
	"packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
