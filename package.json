{
	"name": "imgproxy-aws-cdk",
	"version": "0.1.0",
	"type": "module",
	"targets": {
		"url-rewrite": {
			"source": "functions/url-rewrite/index.ts",
			"distDir": ".dist/functions/url-rewrite",
			"context": "node",
			"isLibrary": true,
			"outputFormat": "esmodule",
			"includeNodeModules": false,
			"scopeHoist": true,
			"sourceMap": false,
			"optimize": true,
			"engines": {
				"node": ">= 20"
			}
		},
		"main": false
	},
	"bin": {
		"imgproxy": "./bin/imgproxy.js"
	},
	"scripts": {
		"watch": "tsc -w",
		"test": "vitest",
		"cdk": "cdk",
		"typecheck": "tsc --noEmit",
		"init-json": "rm -rf functions/url-rewrite/imgproxy-indexed-options.json functions/url-rewrite/imgproxy-option-priority.json && tsx ./functions/url-rewrite/imgproxy-options.ts",
		"build": "pnpm run init-json && pnpm run typecheck && rm -rf ./.build && pnpm run build:parcel",
		"build:swc": "swc ./functions/url-rewrite/index.ts --out-dir ./.build --config-file .swcrc",
		"build:parcel": "parcel build",
		"build:vite": "vite build"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@parcel/config-default": "^2.13.3",
		"@parcel/plugin": "^2.13.3",
		"@pilchard/aws-cloudfront-function": "link:../aws-cloudfront-function",
		"@types/aws-lambda": "^8.10.147",
		"@types/node": "22.13.5",
		"aws-cdk": "2.1001.0",
		"cloudfront": "link:functions/url-rewrite/__mocks__/cloudfront",
		"dotenv": "^16.4.7",
		"parcel": "^2.13.3",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"typescript": "5.8.2",
		"vite": "^6.2.1",
		"vitest": "^3.0.8"
	},
	"dependencies": {
		"aws-cdk-lib": "2.182.0",
		"constructs": "^10.4.2",
		"source-map-support": "^0.5.21",
		"cloudfront": "link:functions/url-rewrite/__mocks__/cloudfront"
	},
	"packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
